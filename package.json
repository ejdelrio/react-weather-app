{
  "name": "react-weather-app",
  "version": "1.0.0",
  "description": "A simple react weather forcast app, includes search bar for city/zip.",
  "main": "index.js",
  "scripts": {
    "watch": "webpack-dev-server --inline --hot",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "con": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "webpack --mode development ./src/index.js --output ./dist/main.js",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madhubs/react-weather-app.git"
  },
  "author": "Madeline Stevens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madhubs/react-weather-app/issues"
  },
  "homepage": "https://github.com/madhubs/react-weather-app#readme",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "sass-loader": "^7.1.0",
    "superagent": "^4.0.0-beta.5",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.23.1",
    "webpack-dev-server": "^3.1.10"
  },
  "devDependencies": {
    "webpack-cli": "^3.1.2"
  }
}
